{# Purpose of this template: Display files within an external context #}
{% for file in items %}
    <h3>{{ file|rkdownloadmodule_formattedTitle }}</h3>
    <p><a href="{{ path('rkdownloadmodule_file_display', {'id': file.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
